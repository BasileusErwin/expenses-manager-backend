{
  "files": [
    "src/server.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "types": [
      "reflect-metadata"
    ],
    "module": "commonjs",
    "noImplicitAny": false,
    "target": "es2021",
    "lib": [
      "es2021"
    ],
    "outDir": "dist",
    "rootDir": "",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "removeComments": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": "src",
    "paths": {
      "config/*": [
        "config/*"
      ],
      "constrollers/*": [
        "constrollers/*"
      ],
      "enums/*": [
        "enums/*"
      ],
      "helpers/*": [
        "helpers/*"
      ],
      "lib/*": [
        "lib/*"
      ],
      "models/*": [
        "models/*"
      ],
      "routes/*": [
        "routes/*"
      ],
      "services/*": [
        "services/*"
      ],
      "types/*": [
        "types/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "validations/*": [
        "validations/*"
      ],
      "src/*": [
        "*"
      ],
      "*": [
        "../node_modules/@types/*",
        "types/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
